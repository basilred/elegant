block('grid')(
    content()(function () {
        var res = [];
        for (var i = 1; i < 9; i++) {
            res.push({
                block : 'image',
                mix : { block : 'grid', elem : 'image' },
                url : '../../common.blocks/grid/grid' + i + '.jpg'
            });
        }
        return res;
    })
);
